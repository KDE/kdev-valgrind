#, kde-format
msgid "Pages as heap"
msgstr "Küme olarak sayfalar"

#. i18n: ectx: property (text), widget (KMessageWidget, messageWidget)
#: tools/massif/massif_configpage.ui:191
#, kde-format
msgid ""
"<html><head/><body><p>&quot;Pages as heap&quot; cannot be used together with "
